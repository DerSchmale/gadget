bin/linux/bin/dxc
bin/linux/lib/libdxcompiler.so
bin/win32/bin/dxc.exe
bin/win32/bin/dxcompiler.dll
bin/win32/lib/dxclib.lib
bin/win32/lib/dxcompiler.lib

---

